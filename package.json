{
  "name": "client-site-project-setup",
  "description": "Project setup By Masum Rana, utilizing Next.js, Redux, and Axios. To enhance code quality, ESLint, Prettier, and Husky were employed in the development environment.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type:check": "tsc --pretty --noEmit",
    "format:check": "prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint:check": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "test:all": "npm run type:check && npm run format:check && npm run format:fix && npm run lint:check && npm run lint:fix && npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.5",
    "lint-staged": "^15.2.0",
    "next": "^14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^8.0.0",
    "postcss": "^8",
    "prettier": "3.2.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
